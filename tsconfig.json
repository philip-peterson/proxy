{
  "exclude": [
      "src/frontend"
  ],
  "compilerOptions": {
    "module": "ES2022",           // or "ESNext"
    "target": "ES2022",           // or "ESNext"
    "esModuleInterop": true,

    "moduleResolution": "bundler",


    "outDir": "dist",
    "jsxImportSource": "hono/jsx",
    "jsx": "react-jsx",

    "incremental": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
