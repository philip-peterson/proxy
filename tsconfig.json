{
  "compilerOptions": {
    "outDir": "dist",
    "jsxImportSource": "hono/jsx",
    "jsx": "react-jsx",

    "incremental": true,
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
